# üö® Don't change the code below üëá
# print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# üö® Don't change the code above üëÜ

combined_string = name1 + name2 # –∫–æ–Ω–∫–∞—Ç–µ–Ω–∏—Ä—É—é –¥–≤–µ —Å—Ç—Ä–æ–∫–∏-–∏–º–µ–Ω–∏
lower_case_string = combined_string.lower() # –ø–µ—Ä–µ–≤–æ–∂—É –ø–æ–ª—É—á–µ–Ω–Ω—É—é —Å—Ç—Ä–æ–∫—É –≤ –Ω–∏–∂–Ω–∏–π —Ä–µ–≥–∏—Å—Ç—Ä

t = lower_case_string.count('t') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "t" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
r = lower_case_string.count('r') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "r" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
u = lower_case_string.count('u') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "u" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
e = lower_case_string.count('e') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "e" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ

true = t + r + u + e

l = lower_case_string.count('l') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "l" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
o = lower_case_string.count('o') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "o" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
v = lower_case_string.count('v') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "v" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ
e = lower_case_string.count('e') # –ø–æ–¥—Å—á–∏—Ç—ã–≤–∞—é, —Å–∫–æ–ª—å–∫–æ —Ä–∞–∑ –±—É–∫–≤–∞ "e" –≤—Å—Ç—Ä–µ—á–∞–µ—Ç—Å—è –≤ –º–æ–µ–π —Å—Ç—Ä–æ–∫–µ

love = l + o + v + e 

score = int(str(true) + str(love)) 


if score < 10 or score > 90:
    print(f"Your score is {score}, you go together like coke and mentos.")
elif score >= 40 and score <= 50:
    print(f"Your score is {score}, you are alright together.")
else:
    print(f"Your score is {score}.")